{
  "name": "clickhouse-sink-connector",
  "config": {
    "connector.class": "com.clickhouse.kafka.connect.ClickHouseSinkConnector",
    "tasks.max": "3",
    "topics": "nft-minted,trades,royalty-distributions,staking-events,proposals-created,votes-cast,content-uploaded,cdc-events",
    "clickhouse.server.url": "http://clickhouse:8123",
    "clickhouse.server.database": "knowton",
    "clickhouse.server.user": "default",
    "clickhouse.server.password": "",
    "clickhouse.topic2table.map": "nft-minted:nft_transactions,trades:nft_transactions,royalty-distributions:royalty_analytics,staking-events:staking_analytics,proposals-created:governance_analytics,votes-cast:governance_analytics,content-uploaded:content_metrics,cdc-events:cdc_events",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "false",
    "value.converter.schemas.enable": "false",
    "clickhouse.table.name": "nft_transactions",
    "clickhouse.insert.mode": "insert",
    "clickhouse.insert.batch.size": "1000",
    "clickhouse.insert.batch.timeout.ms": "5000",
    "clickhouse.retry.count": "3",
    "clickhouse.retry.backoff.ms": "1000",
    "errors.tolerance": "all",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true",
    "errors.deadletterqueue.topic.name": "dlq-clickhouse-failed",
    "errors.deadletterqueue.topic.replication.factor": "1",
    "errors.deadletterqueue.context.headers.enable": "true",
    "transforms": "unwrap,addTimestamp",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.addTimestamp.type": "org.apache.kafka.connect.transforms.InsertField$Value",
    "transforms.addTimestamp.timestamp.field": "sync_timestamp"
  }
}
