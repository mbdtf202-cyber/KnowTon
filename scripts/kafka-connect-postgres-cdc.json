{
  "name": "postgres-cdc-source-connector",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "tasks.max": "1",
    "database.hostname": "postgres",
    "database.port": "5432",
    "database.user": "knowton",
    "database.password": "knowton_password",
    "database.dbname": "knowton",
    "database.server.name": "knowton_postgres",
    "table.include.list": "public.User,public.Creator,public.Content,public.NFT,public.Transaction,public.RoyaltyPayment,public.Stake,public.Proposal,public.Vote,public.FractionalVault",
    "plugin.name": "pgoutput",
    "publication.name": "knowton_publication",
    "publication.autocreate.mode": "filtered",
    "slot.name": "knowton_replication_slot",
    "slot.drop.on.stop": "false",
    "heartbeat.interval.ms": "10000",
    "heartbeat.action.query": "INSERT INTO public.heartbeat (timestamp) VALUES (NOW())",
    "snapshot.mode": "initial",
    "snapshot.locking.mode": "minimal",
    "snapshot.fetch.size": "10000",
    "incremental.snapshot.chunk.size": "1024",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "false",
    "value.converter.schemas.enable": "false",
    "topic.prefix": "knowton",
    "topic.creation.enable": "true",
    "topic.creation.default.replication.factor": "1",
    "topic.creation.default.partitions": "3",
    "topic.creation.default.cleanup.policy": "delete",
    "topic.creation.default.retention.ms": "604800000",
    "transforms": "route,unwrap",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "knowton_postgres.public.(.*)",
    "transforms.route.replacement": "cdc-$1",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.delete.handling.mode": "rewrite",
    "transforms.unwrap.add.fields": "op,source.ts_ms,source.db,source.table",
    "decimal.handling.mode": "string",
    "time.precision.mode": "adaptive",
    "tombstones.on.delete": "true",
    "provide.transaction.metadata": "true",
    "schema.history.internal.kafka.bootstrap.servers": "kafka:9092",
    "schema.history.internal.kafka.topic": "knowton-schema-history",
    "errors.tolerance": "none",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true"
  }
}
